{
  "name": "webdriver-io",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "wdio:run": "wdio run ./wdio.conf.js",
    "appium:run": "appium --port 3000 --address localhost --allow-insecure chromedriver_autodownload,adb_shell",
    "allure:generate": "npx allure generate allure-results --clean -o allure-report",
    "allure:open": "npx allure open allure-report",
    "allure:generate:open": "npm run allure:generate && npm run allure:open",
    "lint": "eslint ./src ./tests --ext .js",
    "prettier": "prettier --write './**/*.{js,json,md}'"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.12.6",
    "@wdio/appium-service": "^9.12.7",
    "@wdio/cli": "^9.12.7",
    "@wdio/local-runner": "^9.12.7",
    "@wdio/mocha-framework": "^9.12.6",
    "@wdio/spec-reporter": "^9.12.6",
    "allure-commandline": "^2.34.0",
    "appium": "^2.18.0",
    "appium-uiautomator2-driver": "^4.2.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-wdio": "^9.9.1",
    "mocha": "^11.1.0",
    "pino-pretty": "^13.0.0",
